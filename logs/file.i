	    local rt = {}
    	string.gsub(str, '[^]' .. p .. ']+', function (w) table.insert(rt, w) end)
    	return rt
    end
    local function absolute(path)
        local str = path:string()
        local list = split(str, '\\')
        local i = #list 
        local result = {}
        while i > 0 do 
            local s = list[i]
            if s ~= '..' and s ~= '.' then 
                table.insert(result, 1, s)
            end
            if s == '..' then 
                i = i - 1
            end 
            i = i - 1
        end
        table.insert(result, '')
        return table.concat(result, '\\')
    end
